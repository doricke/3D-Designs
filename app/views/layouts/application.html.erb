<!DOCTYPE html>
<html>
<head>
  <title>MetaFluidics</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag 'menu' %>
  <%= stylesheet_link_tag 'jquery-ui-1.10.3.custom.min' %>
  <%= javascript_include_tag "jquery-1.9.1.js" %>
  <%= javascript_include_tag "jquery-ui-1.10.3.custom.min" %>
  <%= javascript_include_tag 'jquery.dataTables.min' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render :partial => "layouts/title" %>
  <br />
<script type="text/javascript" charset="utf-8">
  $(document).ready(function() {
    $('.dataTable').dataTable({
      "bJQueryUI": true,
      "sPaginationType": "full_numbers"
      });
   });
</script>
<%= yield %>
</body>
</html>
